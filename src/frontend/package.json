{
  "name": "frank-flow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ../../target/frontend/stats.json",
    "build:dev": "ng build --configuration=dev",
    "watch": "ng build --configuration=dev --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "npx cypress open",
    "prettier": "prettier -w ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.1",
    "@angular/common": "^11.2.1",
    "@angular/compiler": "^11.2.1",
    "@angular/core": "^11.2.1",
    "@angular/forms": "^11.2.1",
    "@angular/platform-browser": "^11.2.1",
    "@angular/platform-browser-dynamic": "^11.2.1",
    "@angular/router": "^11.2.1",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "cytoscape": "^3.19.0",
    "dagre": "^0.8.5",
    "jqwidgets-ng": "^12.0.0",
    "jsplumb": "^2.15.5",
    "monaco-editor": "^0.22.3",
    "ngx-smart-modal": "^7.4.1",
    "ngx-toastr": "^13.2.0",
    "rxjs": "~6.6.0",
    "sax": "^1.2.4",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.1",
    "@angular/cli": "^11.2.1",
    "@angular/compiler-cli": "^11.2.1",
    "@types/cytoscape": "^3.14.15",
    "@types/dagre": "^0.7.44",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^12.11.1",
    "@types/sax": "^1.2.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.1.5",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "pre-push": "npm run test -- --browsers=Chrome --no-watch --no-progress"
    }
  }
}
